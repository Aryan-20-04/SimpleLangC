let arr = [5, 2, 9, 1, 6];
print "Original array:", arr;

for (let i = 0; i < length(arr); i = i + 1) {
    for (let j = 0; j < length(arr) - i - 1; j = j + 1) {
        if (arr[j] > arr[j + 1]) {
            let temp = arr[j];
            arr[j] = arr[j + 1];
            arr[j + 1] = temp;
        }
    }
}

print "Sorted array:", arr;